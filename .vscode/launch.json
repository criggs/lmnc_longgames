    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Multiverse Start",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/lmnc_longgames/multiverse/multiverse_game.py",
            "args": [
                "-w",
                "-u"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}